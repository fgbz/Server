<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="phalaenopsis.common.dao.HomePageInfoDao">
	<select id="getChildRegionId" parameterType="Integer" resultType="HomePageCaseInfo">
		select regionname as region from SS_REGION t where parentid = #{regionId}
	</select>
	
	<select id="getPartolChildRegionId" parameterType="Integer" resultType="HomePagePartolInfo">
		select regionname as region from SS_REGION t where parentid = #{regionId}
	</select>
</mapper>